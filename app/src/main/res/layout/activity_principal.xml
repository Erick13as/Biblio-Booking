<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/backgroundmain"
    android:clickable="true"
    tools:context=".Principal">


    <TextView
        android:id="@+id/texto"
        android:layout_width="336dp"
        android:layout_height="51dp"
        android:layout_marginEnd="37dp"
        android:fontFamily="sans-serif-medium"
        android:text="Biblio-Booking"
        android:textAlignment="center"
        android:textColor="#A0FFFF"
        android:textSize="40sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.232" />

    <TextView
        android:id="@+id/texto2"
        android:layout_width="336dp"
        android:layout_height="51dp"
        android:fontFamily="sans-serif-medium"
        android:text="Pedí un cubículo desde donde estás"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.506"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.336" />

    <TextView
        android:id="@+id/texto3"
        android:layout_width="336dp"
        android:layout_height="51dp"
        android:layout_marginStart="38dp"
        android:layout_marginEnd="37dp"
        android:background="#75FAF9F9"
        android:fontFamily="sans-serif-medium"
        android:text="Consulta disponibilidad al instante"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.473" />

    <TextView
        android:id="@+id/texto4"
        android:layout_width="336dp"
        android:layout_height="83dp"
        android:layout_marginStart="38dp"
        android:layout_marginEnd="37dp"
        android:background="#75FAF9F9"
        android:fontFamily="sans-serif-medium"
        android:text="Es fácil y rápido, con tu carnet de estudiante podrás conseguir un espacio cómodo"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="17sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.575" />

    <androidx.appcompat.widget.AppCompatButton
    android:id="@+id/registrarse"
    android:layout_width="158dp"
    android:layout_height="48dp"
    android:layout_marginTop="512dp"
    android:background="@drawable/round_button_background"
    android:fontFamily="sans-serif-medium"
    android:text="Registrarse"
    android:textAlignment="center"
    android:textColor="#FFFFFF"
    android:textSize="12sp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.936"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent" />

<androidx.appcompat.widget.AppCompatButton
    android:id="@+id/iniciarsesion"
    android:layout_width="158dp"
    android:layout_height="48dp"
    android:layout_marginStart="45dp"
    android:layout_marginTop="512dp"
    android:layout_marginEnd="208dp"
    android:background="@drawable/round_button_background"
    android:fontFamily="sans-serif-medium"
    android:text="Iniciar Sesión"
    android:textAlignment="center"
    android:textColor="#FFFFFF"
    android:textSize="12sp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="1.0"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="109dp"
        android:layout_height="115dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.001"
        app:srcCompat="@drawable/logo2" />




</androidx.constraintlayout.widget.ConstraintLayout>